generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique
  version Int
  
  createdAt  DateTime @default(now())
  updatedAt DateTime @updatedAt

  login  String
  password String
}

model Album {
  id String @id @unique
  
  name String
  year Int
  artistId String?
}

model Artist {
  id String @id @unique

  name String
  grammy Boolean
}

